#pragma once

#pragma include "Includes/Configuration.include"
#pragma include "Includes/Structures/Material.struct"

#pragma include "Includes/LightingPipeline.include"

layout(location=0) out vec4 color0;

void process_and_render_material(Material m) {


    vec3 shaded_result = shade_material_from_buffer(m);
    

    color0.xyz = shaded_result;
    color0.w = 1.0;
}



